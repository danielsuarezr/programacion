
#include "stdafx.h"
#include "conio.h"
#include <iostream>
#include "math.h"
using namespace std;
void main()
{
	int i, n;
	cout<<"ingrese N: ";
	cin>>n;
	for (i=2;i<=n;i=i+2)
		cout<<i<<"\t";
	i=2;
	cout<<"\n";
		while (i<=n)
		{
			cout<<i<<"\t";
				i=i+2;
		}
		cout<<"\n";
		cout<<"ingrese N: ";
	cin>>n;
	for (i=1;i<=n;i=i+2)
		cout<<i<<"\t";
	i=1;
	cout<<"\n";
		while (i<=n)
		{
			cout<<i<<"\t";
				i=i+2;
		}

	getch();

}

